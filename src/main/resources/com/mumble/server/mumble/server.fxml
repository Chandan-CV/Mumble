<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.text.Font?>
<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml" style="-fx-background-color: #fdfd96;"
      fx:controller="com.mumble.server.mumble.ServerControllers">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>
    <Label text="Mumble">
        <font>
            <Font size="20.0" />
        </font>
    </Label>
    <Label fx:id="welcomeText">
    <font>
        <Font size="24.0" />
    </font>
    </Label>
    <Button text="Start server" onAction="#onHelloButtonClick" fx:id="serverButton" />
    <Button text="get QR" onAction="#GetQRButtonClicked"/>
    <Button text="get server status" onAction="#getStatusButtonClicked"/>
    <Button text="get result" onAction="#GetResultButtonClicked"/>
</VBox>